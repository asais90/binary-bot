<xml id="toolbox" style="display: none">
	<category name="Logic" colour="#2a3052" i18n-text="Logic">
		<block type="controls_if"></block>
		<block type="logic_compare"></block>
		<block type="logic_operation"></block>
		<block type="logic_negate"></block>
		<block type="logic_boolean"></block>
		<block type="logic_null"></block>
		<block type="logic_ternary"></block>
	</category>
	<category name="Math" colour="#2a3052" i18n-text="Math">
		<block type="math_number">
		<field name="NUM">1</field>
		</block>
		<block type="math_arithmetic">
		<value name="A">
		<shadow type="math_number">
		<field name="NUM">1</field>
		</shadow>
		</value>
		<value name="B">
		<shadow type="math_number">
		<field name="NUM">1</field>
		</shadow>
		</value>
		</block>
		<block type="math_single">
		<value name="NUM">
		<shadow type="math_number">
		<field name="NUM">9</field>
		</shadow>
		</value>
		</block>
		<block type="math_trig">
		<value name="NUM">
		<shadow type="math_number">
		<field name="NUM">45</field>
		</shadow>
		</value>
		</block>
		<block type="math_constant"></block>
		<block type="math_number_property">
		<value name="NUMBER_TO_CHECK">
		<shadow type="math_number">
		<field name="NUM">0</field>
		</shadow>
		</value>
		</block>
		<block type="math_change">
		<value name="DELTA">
		<shadow type="math_number">
		<field name="NUM">1</field>
		</shadow>
		</value>
		</block>
		<block type="math_round">
		<value name="NUM">
		<shadow type="math_number">
		<field name="NUM">3.1</field>
		</shadow>
		</value>
		</block>
		<block type="math_on_list"></block>
		<block type="math_modulo">
		<value name="DIVIDEND">
		<shadow type="math_number">
		<field name="NUM">64</field>
		</shadow>
		</value>
		<value name="DIVISOR">
		<shadow type="math_number">
		<field name="NUM">10</field>
		</shadow>
		</value>
		</block>
		<block type="math_constrain">
		<value name="VALUE">
		<shadow type="math_number">
		<field name="NUM">50</field>
		</shadow>
		</value>
		<value name="LOW">
		<shadow type="math_number">
		<field name="NUM">1</field>
		</shadow>
		</value>
		<value name="HIGH">
		<shadow type="math_number">
		<field name="NUM">100</field>
		</shadow>
		</value>
		</block>
		<block type="math_random_int">
		<value name="FROM">
		<shadow type="math_number">
		<field name="NUM">1</field>
		</shadow>
		</value>
		<value name="TO">
		<shadow type="math_number">
		<field name="NUM">100</field>
		</shadow>
		</value>
		</block>
		<block type="math_random_float"></block>

	</category>
	<category name="Text" colour="#2a3052" i18n-text="Text">>
		<block type="text"></block>
		<block type="text_join"></block>
		<block type="text_append">
		<value name="TEXT">
		<shadow type="text"></shadow>
		</value>
		</block>
		<block type="text_length">
		<value name="VALUE">
		<shadow type="text">
		<field name="TEXT">abc</field>
		</shadow>
		</value>
		</block>
		<block type="text_isEmpty">
		<value name="VALUE">
		<shadow type="text">
		<field name="TEXT"></field>
		</shadow>
		</value>
		</block>
		<block type="text_indexOf">
		<value name="VALUE">
		<block type="variables_get">
		<field name="VAR">text</field>
		</block>
		</value>
		<value name="FIND">
		<shadow type="text">
		<field name="TEXT">abc</field>
		</shadow>
		</value>
		</block>
		<block type="text_charAt">
		<value name="VALUE">
		<block type="variables_get">
		<field name="VAR">text</field>
		</block>
		</value>
		</block>
		<block type="text_getSubstring">
		<value name="STRING">
		<block type="variables_get">
		<field name="VAR">text</field>
		</block>
		</value>
		</block>
		<block type="text_changeCase">
		<value name="TEXT">
		<shadow type="text">
		<field name="TEXT">abc</field>
		</shadow>
		</value>
		</block>
		<block type="text_trim">
		<value name="TEXT">
		<shadow type="text">
		<field name="TEXT">abc</field>
		</shadow>
		</value>
		</block>
		<block type="text_print">
		<value name="TEXT">
		<shadow type="text">
		<field name="TEXT">abc</field>
		</shadow>
		</value>
		</block>
		<block type="text_prompt_ext">
		<value name="TEXT">
		<shadow type="text">
		<field name="TEXT">abc</field>
		</shadow>
		</value>
		</block>
	</category>
	<category name="List" colour="#2a3052" i18n-text="List">>
		<block type="lists_create_with">
		<mutation items="0"></mutation>
		</block>
		<block type="lists_create_with"></block>
		<block type="lists_repeat">
		<value name="NUM">
		<shadow type="math_number">
		<field name="NUM">5</field>
		</shadow>
		</value>
		</block>
		<block type="lists_length"></block>
		<block type="lists_isEmpty"></block>
		<block type="lists_indexOf">
		<value name="VALUE">
		<block type="variables_get">
		<field name="VAR">list</field>
		</block>
		</value>
		</block>
		<block type="lists_getIndex">
		<value name="VALUE">
		<block type="variables_get">
		<field name="VAR">list</field>
		</block>
		</value>
		</block>
		<block type="lists_setIndex">
		<value name="LIST">
		<block type="variables_get">
		<field name="VAR">list</field>
		</block>
		</value>
		</block>
		<block type="lists_getSublist">
		<value name="LIST">
		<block type="variables_get">
		<field name="VAR">list</field>
		</block>
		</value>
		</block>
		<block type="lists_split">
		<value name="DELIM">
		<shadow type="text">
		<field name="TEXT">,</field>
		</shadow>
		</value>
		</block>
	</category>
	<category name="Variable" id="Variables" colour="#2a3052" custom="VARIABLE" i18n-text="Variable">></category>
	<category name="Functions" id="Functions" colour="#2a3052" custom="PROCEDURE" i18n-text="Functions"></category>
	<category name="Binary" colour="#2a3052" i18n-text="Binary">
		<category name="Trade Definition" colour="#2a3052" i18n-text="Trade Definition">
			<block type="trade"></block>
			<!--Markets-->
			<category name="Trade Types" colour="#2a3052" i18n-text="Trade Types">
				<category name="Up/Down" colour="#2a3052" i18n-text="Up/Down">
					<block type="risefall"></block>
					<block type="higherlower">
						<value name="BARRIEROFFSET">
							<block type="barrier_offset"></block>
						</value>
					</block>
				</category>
				<category name="Touch/No Touch" colour="#2a3052" i18n-text="Touch/No Touch">
					<block type="touchnotouch">
						<value name="BARRIEROFFSET">
							<block type="barrier_offset"></block>
						</value>
					</block>
				</category>
				<category name="In/Out" colour="#2a3052" i18n-text="In/Out">
					<block type="endsinout">
						<value name="BARRIEROFFSET">
							<block type="barrier_offset">
								<field name="BARRIEROFFSETTYPE_LIST">+</field>
							</block>
						</value>
						<value name="SECONDBARRIEROFFSET">
							<block type="barrier_offset">
								<field name="BARRIEROFFSETTYPE_LIST">-</field>
							</block>
						</value>
					</block>
					<block type="staysinout">
						<value name="BARRIEROFFSET">
							<block type="barrier_offset">
								<field name="BARRIEROFFSETTYPE_LIST">+</field>
							</block>
						</value>
						<value name="SECONDBARRIEROFFSET">
							<block type="barrier_offset">
								<field name="BARRIEROFFSETTYPE_LIST">-</field>
							</block>
						</value>
					</block>
				</category>
				<category name="Asians" colour="#2a3052" i18n-text="Asians">
					<block type="asians"></block>
				</category>
				<category name="Digits" colour="#2a3052" i18n-text="Digits">
					<block type="matchesdiffers"></block>
					<block type="evenodd"></block>
					<block type="overunder"></block>
				</category>
			</category>
			<category name="Trade Block" colour="#2a3052" i18n-text="Trade Block">
				<block type="trade"></block>
			</category>
		</category>
		<category name="Before Purchase" colour="#2a3052" i18n-text="Before Purchase">
			<block type="before_purchase"></block>
			<block type="tick"></block>
			<block type="ticks"></block>
			<block type="ohlc"></block>
			<block type="ohlc_values"></block>
			<block type="read_ohlc"></block>
			<block type="read_ohlc_obj"></block>
			<block type="check_direction"></block>
			<block type="purchase"></block>
			<block type="payout"></block>
			<block type="ask_price"></block>
		</category>
		<category name="During Purchase" colour="#2a3052" i18n-text="During Purchase">
			<block type="during_purchase"></block>
			<block type="check_sell"></block>
			<block type="sell_price"></block>
			<block type="sell_at_market"></block>
		</category>
		<category name="After Purchase" colour="#2a3052" i18n-text="After Purchase">
			<block type="after_purchase"></block>
			<block type="contract_check_result"></block>
			<block type="read_details"></block>
			<block type="trade_again"></block>
		</category>
		<category name="Tools" colour="#2a3052" i18n-text="Tools">
			<category name="Misc." colour="#2a3052" i18n-text="Misc.">
				<block type="barrier_offset"></block>
				<block type="balance"></block>
				<block type="total_profit"></block>
				<block type="total_runs"></block>
				<block type="notify"></block>
				<block type="block_holder"></block>
			</category>
			<category name="Indicators" colour="#2a3052" i18n-text="Indicators">
				<block type="sma"></block>
				<block type="ema"></block>
			</category>
		</category>
	</category>
</xml>
